package io.accelerate.solutions.CHK;

public class ProductCatalog {
    HashMap<Character, Product> ProductCatalogMap = new HashMap<Character, Product>;

    public Product(char name, int price, int amount) {
        this.name = name;
        this.price = price;
        this.amount = amount;
    }

    public char getName() {
        return name;
    }

    public int getPrice() {
        return price;
    }

    public int getAmount() {
        return amount;
    }
}
